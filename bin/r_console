#!/usr/bin/env ruby
require File.expand_path('../../lib/r_type', __FILE__)

begin
  require 'pry'
rescue LoadError => e
  require 'irb'
end

if defined? Pry
  RType::R.run do
    Pry.start binding, print: ->(output, value){ output.puts "=> #{value.inspect}" }
  end
else
  RType::R.run do
    IRB.start
  end
end
